  <div id="container_main">

    <h2>Notes!!</h2>

    <% @article_list.each do |article| %>

        <div class="row">
          <div class="col s12 m7">
            <div class="card">
              <div class="card-content">
                <h4><%= article.title %></h4>
                <p><%= article.article %></p>
              </div>
              <div class="card-action">
                <%= link_to '編集する', edit_articles_path(id: article.id) %>
              </div>
            </div>
          </div>
        </div>

    <% end %>

  </div>

